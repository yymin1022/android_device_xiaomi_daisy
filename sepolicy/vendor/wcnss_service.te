get_prop(wcnss_service, wifi_prop);
set_prop(wcnss_service, wifi_prop);
set_prop(wcnss_service, serialno_prop);

allow wcnss_service wifi_data_file:dir search;

allow wcnss_service init:unix_stream_socket connectto;
